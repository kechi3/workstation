FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "1";
    object      DT;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 0.000117;

boundaryField
{
    face
    {
        type            calculated;
        value           uniform 0.000117;
    }
    outlet
    {
        type            calculated;
        value           uniform 0.000117;
    }
    wall
    {
        type            calculated;
        value           uniform 0.000117;
    }
}


